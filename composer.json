{
    "name": "yahiru/ca-factory",
    "description": "generate skeleton classes for clean architecture",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "yuhei yoshida",
            "email": "yahiru1121@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "phpstan/phpstan": "^0.11.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "Yahiru\\CAFactory\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit tests && ./vendor/bin/phpstan analyse"
    }
}
